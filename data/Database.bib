@article{Magenes2010,
abstract = {An extended experimental programme on seismic behaviour of undressed double leaf stone masonry has been carried out at the Eucentre and at the University of Pavia. Within the experimental programme, which also includes shake-table tests on three full scale prototype buildings, a set of complementary tests on single walls has been performed to characterize the mechanical properties of this type of masonry. Among other, they include in-plane cyclic shear tests on large wall specimens. Two levels of axial load and two slenderness ratios have been considered in order to obtain both rocking and shear failure modes. Useful information for this masonry typology has been collected from the tests: strength parameters, displacement capacity and hysteretic energy dissipation properties. Attention has been devoted to the damage evolution during the test, by recording position and width of cracks for increasing values of in-plane drift.},
author = {Magenes, Guido and Penna, Andrea and Galasco, Alessandro and da Par{\'{e}}, Marco},
file = {:V\:/03_Projects/04_Basel_Project/04_Drift_capacity/02_Database/00_Lit_tests/4 G.Magenes A.Galasco 2009-In-plane cyclic shear tests of undressed double-leaf stone masonry panels.pdf:pdf},
journal = {8th International Masorny Conference},
keywords = {Stone masonry,deformation capacity,energy dissipation,in-plane cyclic testing},
pages = {1--10},
title = {{In-plane cyclic shear tests of undressed double leaf stone masonry panels}},
