isbn = {0817683534},
issn = {13598368},
journal = {Composites Part B: Engineering},
keywords = {A. Fibres,B. Mechanical properties,D. Mechanical testing},
number = {7-8},
pages = {887--901},
title = {{In-plane shear performance of masonry panels strengthened with FRP}},
volume = {38},
year = {2007}
}
