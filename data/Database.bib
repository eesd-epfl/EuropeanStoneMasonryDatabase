@article{Silva2012a,
abstract = {Contributors to this issue of the journal present their views on various aspects of testing and achievement of black Americans. The articles contain evaluative opinions on the current status of testing issues and black achievement from both historical and future perspectives. Ingeneral, there is agreement that improvement is needed in the quality of tests as well as the testing services provided to the public. Differences exist, however, with respect to the specific approach advocated for remedying problems involving tests. Several contributors recommend aresearch-evaluation approach for resolving critical issues relative to the testing and achievement of black Americans. Other recommendations include a demand for a testing moratorium, the improvement and increased use of criterion-referenced measures, and proposals that appeal for truth in testingand "sunset" legislation. A commonality shared by all of the contributors is the objective of creating an acceptable degree of public accountability on the part of the testing corporations and professionals in psychology and education.    },
author = {Silva, Bruno Luis},
file = {:V\:/03_Projects/04_Basel_Project/04_Drift_capacity/02_Database/00_Lit_tests/2 B.da Silva 2012-Diagnosis and strengthening of historical masonry structures .pdf:pdf},
journal = {PhD Thesis},
title = {{Diagnosis and Strengthening of Historical Masonry Structures: Numerical and Experimental Analysis}},
year = {2012}
}


